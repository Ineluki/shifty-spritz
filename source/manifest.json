{
  "manifest_version": 2,
  "name": "spritzify",
  "description": "Easy to use Spritz style reader",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Click me!"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/application.css"],
      "js": ["js/application.js"]
    }
  ],
  "web_accessible_resources": [
    "index.html"
  ],
  "permissions": ["tabs", "<all_urls>"]
}